<h1>Edit a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_id %>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %>
  <br />

  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />

  <%= f.submit %>
<% end %>
